{
  "name": "@hikoui/storybook",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm storybook:build",
    "clean": "rimraf storybook-static .turbo",
    "dev": "pnpm storybook",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "sort-package-json": "pnpm exec sort-package-json",
    "sort-package-json:check": "pnpm exec sort-package-json --check",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "typescheck": "tsc --noEmit",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.27.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/experimental-addon-test": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import-x": "^4.12.2",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-storybook": "^0.12.0",
    "eslint-plugin-turbo": "^2.5.3",
    "playwright": "^1.52.0",
    "sort-package-json": "^3.0.0",
    "storybook": "^8.6.14",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.4"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=20"
  }
}
