{
  "dark": {
    "colors": {
      "$type": "color",
      "layout": {
        "background": {
          "$value": "var(--color-black)"
        },
        "background-contrast": {
          "$value": "var(--color-hiko-neutral-900)"
        },
        "background-high-contrast": {
          "$value": "var(--color-hiko-neutral-700)"
        },
        "foreground": {
          "$value": "var(--color-white)"
        },
        "foreground-contrast": {
          "$value": "var(--color-hiko-neutral-100)"
        },
        "foreground-high-contrast": {
          "$value": "var(--color-hiko-neutral-300)"
        },
        "content": {
          "$value": "var(--color-white)"
        },
        "content-heading": {
          "$value": "var(--color-hiko-neutral-100)"
        },
        "content-label": {
          "$value": "var(--color-hiko-neutral-300)"
        },
        "content-muted": {
          "$value": "--alpha(var(--color-white) / 50%)"
        }
      }
    }
  }
}
